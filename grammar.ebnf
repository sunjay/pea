program ::= decl*

decl ::= func_decl

func_decl ::= "fn" ident "(" ")" block

block ::= "{" stmt* "}"

stmt ::= println_stmt | var_decl_stmt | expr_stmt
println_stmt ::= "println" "!" "(" expr ")" ";"
var_decl_stmt ::= "let" ident "=" expr ";"
expr_stmt ::= expr ";"

expr ::= call | ident | integer_literal | byte_str
call ::= expr "(" ")"
